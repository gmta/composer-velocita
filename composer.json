{
    "name": "isaac/composer-velocita",
    "description": "Fast and reliable Composer package downloads by using Velocita",
    "keywords": ["composer", "cache", "performance", "speed", "availability", "velocita"],
    "type": "composer-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Jelle Raaijmakers",
            "email": "jelle.raaijmakers@isaac.nl",
            "homepage": "https://www.isaac.nl"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "composer-plugin-api": "^2.0"
    },
    "require-dev": {
        "composer/composer": "^2.0",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7",
        "object-calisthenics/phpcs-calisthenics-rules": "^3.8",
        "phpcompatibility/php-compatibility": "^9.3.5",
        "phpstan/phpstan": "^0.12.36",
        "slevomat/coding-standard": "^6.3.10",
        "symfony/flex": "^1.9.1"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "ISAAC\\Velocita\\Composer\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "class": "ISAAC\\Velocita\\Composer\\VelocitaPlugin"
    }
}
